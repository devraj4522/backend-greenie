{
  "version": 2,
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"  
    },
     "commands": [
      "pip install --no-cache-dir -r requirements.txt",
      "pip install --no-cache-dir psycopg2-binary==2.9.5"
     ]
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ]
}
